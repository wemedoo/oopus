@using sReportsV2.DTOs.Form.DataOut

@model List<FormDataOut>

@foreach(FormDataOut form in Model)
{
    <div class="single-form-list-element" data-id="@form.Id">
        <div class="pin-icon @((ViewBag.UserCookieData.SuggestedForms as List<string>).Contains(form.Id) ? "pinned" : "")" id="pinned-icon" onclick="pinOrUnpinSuggestedForm(event, '@form.Id')">
        </div>
        <div class="title">
            @form.Title
        </div>
        <div class="select-button unselect">
            @sReportsV2.Cache.Resources.TextLanguage.Unselect
        </div>
        <div class="select-button">
            @sReportsV2.Cache.Resources.TextLanguage.Select
        </div>
    </div>
}
