@using sReportsV2.DTOs.Form.DataOut.Tree

@model TreeDataOut

@if (Model.Forms.Count() == 0)
{
    <div class="no-result-content">
        <img alt="" class="margin-" src="/css/img/icons/no_results.svg">
        <br>
        <div class="no-result-text">@sReportsV2.Cache.Resources.TextLanguage.No_result_found!</div>
    </div>
}
else
{
    <div class="tree-element-style" id="thesaurusTreeItems">
        @await Html.PartialAsync("Thesaurus/FormThesaurusAppearanceTreePartial")
    </div>
}